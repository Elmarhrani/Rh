{% extends 'base.html.twig' %}

{% block CONTENT %}

    {% block  modalSlideUp %}
        {% include 'adefault/modalSlideUp.html.twig' %}
    {% endblock %}

    <div  id="modalFillIn" tabindex="-1" role="dialog" aria-hidden="true" {% if modal is defined and modal %} class="modal fade fill-in in" style="display: block; padding-right: 15px;" {% else %} class="modal fade fill-in" style="display: none; padding-right: 15px;" {% endif %}>
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
            <i class="pg-close"></i>
        </button>
        <div class="modal-dialog ">
            <div class="modal-content">
                <div class="modal-header">
                    <!-- <h5 class="text-left p-b-5"><span class="semi-bold">News letter</span> signup</h5> -->
                </div>
                <div class="modal-body"  data-prototype="{% block modal_body_data_prototype %}{% endblock %}">
                    {% block modal_body %}{% endblock %}
                    <!-- <p class="text-right sm-text-center hinted-text p-t-10 p-r-10">What is it? Terms and conditions</p> -->
                </div>
                <div class="modal-footer">
                </div>
            </div>
            <!-- /.modal-content -->
        </div>
        <!-- /.modal-dialog -->
    </div>
    {% block DataTable %}

    {% endblock %}
{% endblock %}

{% block QUICKVIEW %}
    <div id="quickview" class="quickview-wrapper" data-pages="quickview">

        <a class="btn-link quickview-toggle" id="btn-quickview" data-toggle-element="#quickview" data-toggle="quickview"><i class="pg-close"></i></a>
        <div class="tab-content" style="top: 60px">
            <div class="tab-pane fade in active no-padding " id="quickview-chat" style="max-height: 100vh;">
                <div>
                    <div class="panel panel-transparent">

                        <ul class="nav nav-tabs nav-tabs-linetriangle hidden-sm hidden-xs" data-init-reponsive-tabs="dropdownfx">
                            {% block quickviewTabs %}

                            {% endblock quickviewTabs %}
                        </ul>

                        <div class="tab-content scrollable">
                            <div class="tab-pane active" id="home">

                            </div>
                            <div class="tab-pane" id="profile">

                            </div>
                            <div class="tab-pane" id="messages">

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
{% endblock %}

{% block stylesheets %}
    {% if calendar is defined %}
        <link href="{{ asset('assets/css/formStyle.css') }}" rel="stylesheet" type="text/css"/>
    {% endif %}
{% endblock %}


